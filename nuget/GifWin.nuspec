<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>GifWin</id>
        <version>$version$</version>
        <authors>Bojan Rajkovic,Eric Maupin</authors>
        <projectUrl>https://github.com/bojanrajkovic/GifWin</projectUrl>
        <iconUrl>https://s3.amazonaws.com/gifwin-releases/gifwin.ico</iconUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>GifWin is a reimagining of the excellent GifWit, but for Windows users, by Windows users.</description>
        <language>en-US</language>
    </metadata>
    <files>
        <file src="..\src\GifWin\bin\Release\x64\sqlite3.dll" target="lib\net45\x64\sqlite3.dll" />
        <file src="..\src\GifWin\bin\Release\x86\sqlite3.dll" target="lib\net45\x86\sqlite3.dll" />
        <file src="..\src\GifWin\bin\Release\*.exe" target="lib\net45" />
        <file src="..\src\GifWin\bin\Release\*.dll" target="lib\net45" />
        <file src="..\src\GifWin\bin\Release\*.pdb" target="lib\net45" />
        <file src="..\src\GifWin\bin\Release\GifWin.exe.config" target="lib\net45\GifWin.exe.config" />
    </files>
</package>
